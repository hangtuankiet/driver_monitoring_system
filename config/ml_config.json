{
  "ml_config": {
    "detection_model_version": "yolov11",
    "classification_backbone": "efficientnet_b0",
    "device": "auto",
    "inference_batch_size": 1,
    "model_warmup": true,
    "optimize_for_inference": true
  },
  "detection": {
    "confidence_threshold": 0.5,
    "eye_closure_time": 2.0,
    "yawn_duration": 1.5,
    "alert_cooldown": 3.0
  },
  "performance": {
    "max_fps": 30,
    "frame_skip": 1,
    "queue_size": 2,
    "thread_priority": "normal"
  },
  "ui": {
    "update_interval": 50,
    "display_confidence": true,
    "display_fps": true,
    "log_level": "INFO"
  }
}